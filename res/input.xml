<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="input.xsd">
    <output>
        <name>MD_vtk</name>
        <format>VTK</format>
        <frequency>10</frequency>
    </output>
    <param>
        <calc>LJ_FULL</calc>
        <boundaries>
            <boundary_yz_near>INF_CONT</boundary_yz_near>
            <boundary_xz_near>INF_CONT</boundary_xz_near>
            <boundary_xy_near>INF_CONT</boundary_xy_near>
            <boundary_yz_far>INF_CONT</boundary_yz_far>
            <boundary_xz_far>INF_CONT</boundary_xz_far>
            <boundary_xy_far>INF_CONT</boundary_xy_far>
        </boundaries>
        <epsilon>5.0</epsilon>
        <sigma>1.0</sigma>
        <delta_t>0.014</delta_t>
        <t_end>1000.0</t_end>
        <dimensions>3</dimensions>
        <r_cutoff>3.0</r_cutoff>
        <domain>
            <vx>100.0</vx>
            <vy>50.0</vy>
            <vz>50.0</vz>
        </domain>
    </param>
    <particle>
        <position>
            <vx>0.1</vx>
            <vy>0.2</vy>
            <vz>0.3</vz>
        </position>
        <velocity>
            <vx>1.0</vx>
            <vy>1.5</vy>
            <vz>1.7</vz>
        </velocity>
        <m>0.01</m>
    </particle>
    <cuboid>
        <position>
            <vx>1.0</vx>
            <vy>2.0</vy>
            <vz>3.0</vz>
        </position>
        <velocity>
            <vx>0.5</vx>
            <vy>0.7</vy>
            <vz>0.9</vz>
        </velocity>
        <m>2.5</m>
        <count>
            <vx>10</vx>
            <vy>15</vy>
            <vz>20</vz>
        </count>
        <h>0.3</h>
        <b_motion>0.1</b_motion>
    </cuboid>
    <disc>
        <center>
            <vx>-1.3</vx>
            <vy>-2.2</vy>
            <vz>-3.1</vz>
        </center>
        <velocity>
            <vx>-35.8</vx>
            <vy>-2.0</vy>
            <vz>5.0</vz>
        </velocity>
        <m>0.4</m>
        <r>5.9</r>
        <h>1.0</h>
        <b_motion>0.0</b_motion>
    </disc>
</simulation>