<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../res/input.xsd">
    <output>
        <name>Two_Body</name>
        <format>NO_OUT</format>
        <frequency>100</frequency>
    </output>
    <param>
        <calc>LJ_FULL</calc>
        <boundaries>
            <boundary_yz_near>OUTFLOW</boundary_yz_near>
            <boundary_xz_near>OUTFLOW</boundary_xz_near>
            <boundary_xy_near>OUTFLOW</boundary_xy_near>
            <boundary_yz_far>OUTFLOW</boundary_yz_far>
            <boundary_xz_far>OUTFLOW</boundary_xz_far>
            <boundary_xy_far>OUTFLOW</boundary_xy_far>
        </boundaries>
        <epsilon>5.0</epsilon>
        <sigma>1.0</sigma>
        <delta_t>0.0005</delta_t>
        <t_end>20.0</t_end>
        <dimensions>2</dimensions>
        <r_cutoff>3.0</r_cutoff>
        <domain>
            <vx>200.0</vx>
            <vy>200.0</vy>
            <vz>1.0</vz>
        </domain>
    </param>
    <cuboid>
        <position>
            <vx>20</vx>
            <vy>20</vy>
            <vz>0</vz>
        </position>
        <velocity>
            <vx>0</vx>
            <vy>0</vy>
            <vz>0</vz>
        </velocity>
        <m>1.0</m>
        <count>
            <vx>100</vx>
            <vy>80</vy>
            <vz>1</vz>
        </count>
        <h>1.1225</h>
        <b_motion>0.0</b_motion>
    </cuboid>
</simulation>