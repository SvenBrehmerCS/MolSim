<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../res/input.xsd">
    <output>
        <name>Membrane</name>
        <format>VTK</format>
        <frequency>100</frequency>
    </output>
    <param>
        <calc>LJ_MOL</calc>
        <strategy>SERIAL</strategy>
        <boundaries>
            <boundary_yz_near>HARD</boundary_yz_near>
            <boundary_xz_near>HARD</boundary_xz_near>
            <boundary_xy_near>HALO</boundary_xy_near>
            <boundary_yz_far>HARD</boundary_yz_far>
            <boundary_xz_far>HARD</boundary_xz_far>
            <boundary_xy_far>HARD</boundary_xy_far>
        </boundaries>
        <delta_t>0.01</delta_t>
        <t_end>500</t_end>
        <dimensions>3</dimensions>
        <r_c>4.0</r_c>
        <r_l>0.1</r_l>
        <domain>
            <vx>148</vx>
            <vy>148</vy>
            <vz>148</vz>
        </domain>
        <g_grav>-0.001</g_grav>
    </param>
    <membrane>
        <position>
            <vx>15</vx>
            <vy>15</vy>
            <vz>1.5</vz>
        </position>
        <velocity>
            <vx>0</vx>
            <vy>0</vy>
            <vz>0</vz>
        </velocity>
        <count>
            <vx>50</vx>
            <vy>50</vy>
        </count>
        <m>1.0</m>
        <sigma>1.0</sigma>
        <epsilon>1.0</epsilon>
        <h>2.2</h>
        <b_motion>0.0</b_motion>
        <m_stiffness>300</m_stiffness>
        <avg_bond_len>2.2</avg_bond_len>
        <tweezer>
            <force>
                <vx>0.0</vx>
                <vy>0.0</vy>
                <vz>0.8</vz>
            </force>
            <t_end>150</t_end>
            <target>
                <vx>17</vx>
                <vy>24</vy>
            </target>
            <target>
                <vx>17</vx>
                <vy>25</vy>
            </target>
            <target>
                <vx>18</vx>
                <vy>24</vy>
            </target>
            <target>
                <vx>18</vx>
                <vy>25</vy>
            </target>
        </tweezer>
    </membrane>
</simulation>