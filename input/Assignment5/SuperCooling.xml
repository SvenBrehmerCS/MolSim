<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../res/input.xsd">
    <output>
        <name>SuperCooling</name>
        <format>VTK</format>
        <frequency>50</frequency>
    </output>
    <param>
        <calc>LJ_SMOOTH</calc>
        <strategy>SERIAL</strategy>
        <boundaries>
            <boundary_yz_near>PERIODIC</boundary_yz_near>
            <boundary_xz_near>PERIODIC</boundary_xz_near>
            <boundary_xy_near>PERIODIC</boundary_xy_near>
            <boundary_yz_far>PERIODIC</boundary_yz_far>
            <boundary_xz_far>PERIODIC</boundary_xz_far>
            <boundary_xy_far>PERIODIC</boundary_xy_far>
        </boundaries>
        <delta_t>0.001</delta_t>
        <t_end>100</t_end>
        <dimensions>3</dimensions>
        <r_c>2.3</r_c>
        <r_l>1.9</r_l>
        <domain>
            <vx>9.2</vx>
            <vy>9.2</vy>
            <vz>9.2</vz>
        </domain>
        <g_grav>0</g_grav>
    </param>
    <thermo>
        <T_init>3.0</T_init>
        <T_target>0.02</T_target>
        <T_frequency>25</T_frequency>
        <max_delta_T>0.0025</max_delta_T>
    </thermo>
    <checkpoint>equilibrium</checkpoint>
</simulation>