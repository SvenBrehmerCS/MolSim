<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../res/input.xsd">
    <output>
        <name>EquilibriumDatei</name>
        <format>CHECKPOINT</format>
        <frequency>1</frequency>
    </output>
    <param>
        <calc>LJ_FULL</calc>
        <boundaries>
            <boundary_yz_near>HARD</boundary_yz_near>
            <boundary_xz_near>HARD</boundary_xz_near>
            <boundary_xy_near>HARD</boundary_xy_near>
            <boundary_yz_far>HARD</boundary_yz_far>
            <boundary_xz_far>HARD</boundary_xz_far>
            <boundary_xy_far>HARD</boundary_xy_far>
        </boundaries>
        <delta_t>0.0005</delta_t>
        <t_end>0.005</t_end>
        <dimensions>2</dimensions>
        <r_cutoff>3</r_cutoff>
        <domain>
            <vx>303</vx>
            <vy>180</vy>
            <vz>9.0</vz>
        </domain>
        <T_init>0.5</T_init>
        <T_frequency>1000</T_frequency>
        <g_grav>-12.44</g_grav>
    </param>
    <cuboid>
        <position>
            <vx>1.5</vx>
            <vy>2</vy>
            <vz>4.5</vz>
        </position>
        <velocity>
            <vx>0</vx>
            <vy>0</vy>
            <vz>0</vz>
        </velocity>
        <count>
            <vx>250</vx>
            <vy>50</vy>
            <vz>1</vz>
        </count>
        <m>1.0</m>
        <sigma>1.2</sigma>
        <epsilon>1.0</epsilon>
        <h>1.2</h>
        <b_motion>0.0</b_motion>
    </cuboid>
</simulation>