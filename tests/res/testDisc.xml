<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="../../res/input.xsd">
    <output>
        <name>MD_vtk</name>
        <format>VTK</format>
        <frequency>10</frequency>
    </output>
    <param>
        <calc>LJ_FULL</calc>
        <epsilon>5.0</epsilon>
        <sigma>1.0</sigma>
        <delta_t>0.014</delta_t>
        <t_end>1000.0</t_end>
        <dimensions>3</dimensions>
        <r_cutoff>3.0</r_cutoff>
    </param>
    <disc>
        <c_x>0</c_x>
        <c_y>0</c_y>
        <c_z>0</c_z>
        <vel_x>1</vel_x>
        <vel_y>1</vel_y>
        <vel_z>1</vel_z>
        <m>2.5</m>
        <r>1</r>
        <h>1</h>
        <b_motion>0</b_motion>
    </disc>
</simulation>