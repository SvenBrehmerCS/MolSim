<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="../../res/input.xsd">
    <output>
        <name>MD_vtk</name>
        <format>VTK</format>
        <frequency>10</frequency>
    </output>
    <param>
        <calc>LJ_FULL</calc>
        <epsilon>5.0</epsilon>
        <sigma>1.0</sigma>
        <delta_t>0.014</delta_t>
        <t_end>1000.0</t_end>
        <dimensions>3</dimensions>
        <r_cutoff>3.0</r_cutoff>
    </param>
    <cuboid>
        <pos_x>0</pos_x>
        <pos_y>0</pos_y>
        <pos_z>0</pos_z>
        <vel_x>0.5</vel_x>
        <vel_y>0.7</vel_y>
        <vel_z>0.9</vel_z>
        <m>2.5</m>
        <n_x>2</n_x>
        <n_y>2</n_y>
        <n_z>1</n_z>
        <h>1</h>
        <b_motion>0</b_motion>
    </cuboid>
</simulation>