<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../res/input.xsd">
    <output>
        <name>TestNonDefault</name>
        <format>XYZ</format>
        <frequency>5</frequency>
    </output>
    <param>
        <calc>GRAVITY</calc>
        <bound>HALO</bound>
        <epsilon>10</epsilon>
        <sigma>2.0</sigma>
        <delta_t>0.5</delta_t>
        <t_end>500</t_end>
        <dimensions>2</dimensions>
        <r_cutoff>2.0</r_cutoff>
        <domain>
            <vx>25</vx>
            <vy>25</vy>
            <vz>25</vz>
        </domain>
        <p_offset>
            <vx>1</vx>
            <vy>1</vy>
            <vz>1</vz>
        </p_offset>
    </param>
</simulation>