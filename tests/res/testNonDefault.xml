<?xml version="1.0"?>
<simulation xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../res/input.xsd">
    <output>
        <name>TestNonDefault</name>
        <format>XYZ</format>
        <frequency>5</frequency>
    </output>
    <param>
        <calc>GRAVITY</calc>
        <strategy>SLICE</strategy>
        <boundaries>
            <boundary_yz_near>PERIODIC</boundary_yz_near>
            <boundary_xz_near>HARD</boundary_xz_near>
            <boundary_xy_near>HALO</boundary_xy_near>
            <boundary_yz_far>HARD</boundary_yz_far>
            <boundary_xz_far>HALO</boundary_xz_far>
            <boundary_xy_far>OUTFLOW</boundary_xy_far>
        </boundaries>
        <delta_t>0.5</delta_t>
        <t_end>500</t_end>
        <dimensions>2</dimensions>
        <r_c>1</r_c>
        <r_l>0.5</r_l>
        <domain>
            <vx>25</vx>
            <vy>25</vy>
            <vz>25</vz>
        </domain>
        <g_grav>12</g_grav>
    </param>
    <thermo>
        <T_init>35</T_init>
        <T_target>28</T_target>
        <T_frequency>2</T_frequency>
        <max_delta_T>5</max_delta_T>
    </thermo>
</simulation>